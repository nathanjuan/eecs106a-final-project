<?xml version="1.0"?>
<launch>
  <!-- Load the ABB IRB4600 URDF. -->
  <!-- <param name="robot_description" -->
  <!--        command="$(find xacro)/xacro -\-inorder '$(find abb_irb4600_support)/urdf/irb4600_60_205.xacro'" /> -->
  <param name="robot_description"
         command="$(find xacro)/xacro --inorder '$(find abb_robot_sandbox)/urdf/irb4600_ford.urdf'" />

  <!-- Run RViz -->
  <node name="rviz" pkg="rviz" type="rviz"
        args="-d $(find abb_robot_sandbox)/config/test_vis.rviz" output="screen" />

  <!-- Run the rrt test node. -->
  <node name="rrt" pkg="abb_robot_sandbox" type="rrt_demo_v2.py" output="screen" />

</launch>
